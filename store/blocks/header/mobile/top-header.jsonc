{
	"flex-layout.row#top-header-mobile": {
		"children": ["flex-layout.col#welcome-mobile"],
		"props": {
			"blockClass": "top-header",
			"fullWidth": false,
			"paddingTop": 4,
			"paddingBottom": 4,
			"preserveLayoutOnMobile": true
		}
	},
	"flex-layout.col#welcome-mobile": {
		"children": ["rich-text#welcome", "image#tramontina-logo"],
		"props": {
			"blockClass": "welcome",
			"width": "grow",
			"verticalAlign": "middle",
			"horizontalAlign": "center",
			"preventVerticalStretch": true
		}
	}
}
