{
	"container#tab-layout": {
		"title": "Tab Layout",
		"children": ["flex-layout.row#tab-layout"],
		"props": {
			"isActive": true
		}
	},
	"flex-layout.row#tab-layout": {
		"children": ["tab-layout"],
		"props": {
			"blockClass": ["tab-layout", "home"],
			"marginTop": 5,
			"marginBottom": 8
		}
	},
	"tab-layout": {
		"title": "Configuraci√≥n",
		"children": ["tab-list#home", "tab-content#home"],
		"props": {
			"blockClass": "home",
			"defaultActiveTabId": "home1"
		}
	},
	"tab-list#home": {
		"children": [
			"tab-list.item#home1",
			"tab-list.item#home2",
			"tab-list.item#home3",
			"tab-list.item#home4"
		]
	},
	"tab-list.item#home1": {
		"title": "Tab #01",
		"props": {
			"tabId": "home1",
			"label": "Asado",
			"defaultActiveTab": true
		}
	},
	"tab-list.item#home2": {
